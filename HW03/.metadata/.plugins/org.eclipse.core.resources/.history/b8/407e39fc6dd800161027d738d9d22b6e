import java.util.ArrayList;

/**
 * This models the CityHall
 * Here their is an array that holds the police currently in CityHall
 * @author zgoold17
 *
 */

public class CityHall extends Building {
	ArrayList<Person> CityHallPeople = new ArrayList();
	
	public CityHall(String name, String address){
		setName(name);
		setAddress(address);
	}
	
	public void peopleInside(){
		System.out.printf("The people inside are: ");
		for (Person p : CityHallPeople){
			System.out.printf("\n", p);
		}
	}
	
	
}
