import java.util.ArrayList;

public class School extends Building {
	ArrayList<Kid> School = new ArrayList(); // keeps track of kids in school
	
	public School(String name, String address){
		name = getName();
		address = getAddress();
	}
	
	public void addKid(){
		for(int i = 0; i<School.size(); i++){
			Kid a = new Kid();
			School.add(a);
		}
	}
	
}
