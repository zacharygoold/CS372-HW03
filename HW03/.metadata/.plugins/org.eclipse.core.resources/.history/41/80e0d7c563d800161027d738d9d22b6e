import java.util.ArrayList;

/**
 * This models a School
 * School is a building
 * Here you have an array of what kids are in school
 * @author zgoold17
 *
 */

public class School extends Building {
	ArrayList<Person> SchoolPeople = new ArrayList(); // keeps track of people in school
	
	public School(String name, String address, ArrayList<Person> people){
		setName(name);
		setAddress(address);
		SchoolPeople = people;
	}	
}
